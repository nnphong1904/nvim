  "coc.source.emoji.triggerCharacters": [
    ":e"
  ],
  "coc.source.emoji.filetypes": [
    "markdown",
    "typescript", "typescriptreact", "typescript.tsx", "javascript", "javascript.jsx"
  ],
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "typescript.tsx", "javascript", "javascript.jsx", "css", "html"],
  "eslint.format.enable": true,
  "eslint.autoFixOnSave": true,
  "diagnostic.enableHighlightLineNumber": true,
  "coc.preferences.jumpCommand": "edit",
  "diagnostic.warningSign": "**",
  "diagnostic.infoSign": "##",
  "coc.preferences.colorSupport": true,
  "diagnostic.checkCurrentLine": true,
  //explorer
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": "content",
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "pairs.enableCharacters": [
    "(",
    "[",
    "{",
    "'",
    "\"",
    "`"
  ],
  "cSpell.userWords": [
    "Sharees"
  ]
}
